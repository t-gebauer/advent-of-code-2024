;; Executables can not have tests.

(library
  (name lib)
  (modules lib)
  (inline_tests)
  (preprocess (pps ppx_expect)))

(library
  (name days)
  (modules
    day01
    day02)
  (libraries lib)
  (inline_tests)
  (preprocess (pps ppx_expect)))

(executable
  (name runner)
  (modules runner)
  (libraries days))
